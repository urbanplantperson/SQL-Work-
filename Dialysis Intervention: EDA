--- Find most commonly coded procedures in the US, limit top 100 --- 
 SELECT 
  hcpcs_cd,
  COUNT(hcpcs_cd) AS count,
  hcpcs_desc
 FROM medicare_services_2020 
  WHERE rndrng_prvdr_cntry = 'US'
  GROUP BY hcpcs_cd, hcpcs_desc
  ORDER BY count DESC
 LIMIT 100;
